class Position
{
    x = 0
    y = 0
    def move(nx, ny)
    {
        x = nx
        y = ny
    }
}

class Pos3D extends Position
{
    z = 0
    def set(nx, ny, nz)
    {
        x = nx
        y = ny
        z = nz
    }
}

p = Position.new
p.move(3,4)
p.x + p.y
p.x = 10
p.y = 2
p.x + p.y

p3 = Pos3D.new
p3.move(3,4)
p3.x + p3.y + p3.z

p3.set(5, 6, 7)
p3.x + p3.y + p3.z